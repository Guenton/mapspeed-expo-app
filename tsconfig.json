{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "$lib": ["./lib"],
      "$lib/*": ["./lib/*"],
      "@/*": ["./lib/components/*"],
    },
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "nativewind-env.d.ts"],
  "exclude": ["./node_modules"],
}
